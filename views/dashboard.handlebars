<h2>Dashboard</h2>

<div class="container">
  <div class="row">
    <div class="col-xs-4">
      <div class="avatar-div">
        <img src="/public/images/avatar.png" class="avatar" alt="avatar image">
      </div>
      <p class="user-info">
        <h3>{{user.name}}</h3>
        <small>{{user.email}}</small></br>
        <small>{{user.username}}</small>
      </p>
    </div>
      <div class="col-xs-8">
        <input name="search" placeholder="search" class="search-bar">
    </div>
  </div>
</div>
  <div class="row">
    <div class="col-xs-6">
      <div class="user-activity">
        {{>userActivity}}
      </div>
    </div>
      <div class="col-xs-6">
        <div class="bookmarks">
          {{>bookmark}}
        </div>
      </div>
    </div>
  </div>
</div>
